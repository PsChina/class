{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended","google"],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console":"off",
        "no-var":"off",
        "no-undef":"warn",
        "no-unused-vars":"warn"
    },
    "plugins": [
        "html"        
    ]
}